@charset "UTF-8";
/* 颜色变量 */
.box {
  width: 100%;
  height: auto;
}
.box .info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin: 30rpx 30rpx;
  padding: 10rpx 30rpx;
  border-radius: 20rpx;
  background: white;
  font-size: 26rpx;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.box .info .iconfont {
  margin-left: 20rpx;
  font-size: 24rpx;
  color: #666;
}
.box .logistics {
  background: white;
  margin: 30rpx 30rpx;
  border-radius: 20rpx;
}

