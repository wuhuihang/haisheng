@charset "UTF-8";
/* 颜色变量 */
.advertise_display_card {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  background: #fff;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
  width: 642rpx;
  height: 360rpx;
  padding: 20rpx 30rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% auto;
}
.advertise_display_card .left {
  width: 400rpx;
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
}
.advertise_display_card .left .name-box {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 26rpx;
}
.advertise_display_card .left .name-box .avatar {
  width: 124rpx;
  height: 124rpx;
  background: white;
  border-radius: 50%;
  margin-right: 20rpx;
}
.advertise_display_card .left .name-box .name-text {
  height: 90rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-align-content: space-between;
          align-content: space-between;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.advertise_display_card .left .name-box .name-text .name {
  width: 100%;
  font-size: 36rpx;
  font-weight: bold;
}
.advertise_display_card .left .phone-box {
  margin-top: 70rpx;
}
.advertise_display_card .left .phone-box .phone-line {
  font-size: 26rpx;
  font-weight: bold;
  margin-bottom: 38rpx;
}
.advertise_display_card .left .phone-box .phone-line .iconfont {
  display: inline-block;
  width: 48rpx;
  height: 48rpx;
  text-align: center;
  line-height: 48rpx;
  border-radius: 100%;
  background: #333333;
  font-size: 30rpx;
  color: white;
  margin-right: 24rpx;
}
.advertise_display_card .right {
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
          flex-grow: 1;
  font-size: 20rpx;
}
.advertise_display_card .right .qrcode-img {
  width: 186rpx;
  height: 186rpx;
  margin-top: 15rpx;
}
