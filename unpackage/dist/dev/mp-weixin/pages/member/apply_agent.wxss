@charset "UTF-8";
/* 颜色变量 */
.img {
  width: 100%;
  height: 300rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: conver;
  background-position: center center;
  width: 100%;
}
.white-box {
  width: 100%;
  height: auto;
  background: white;
  margin-bottom: 30rpx;
}
.white-box .set-li {
  margin: 0 30rpx;
  height: 95rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-bottom: 1px solid #E6E6E6;
  font-size: 30rpx;
}
.white-box .set-li .right {
  font-size: 26rpx;
  color: #999999;
  text-align: right;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.white-box .set-li:last-child {
  border: none;
}
.submit {
  width: 236rpx;
  height: 74rpx;
  background: #ff9e97;
  border-radius: 37rpx;
  font-size: 30rpx;
  color: white;
  text-align: center;
  line-height: 74rpx;
  margin: 150rpx auto;
  margin-bottom: 0;
}

