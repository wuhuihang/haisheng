<view class="box"><view class="mybox"><block wx:for="{{withdrawList}}" wx:for-item="item" wx:for-index="k" wx:key="k"><view data-event-opts="{{[['tap',[['chooseWithdraw',['$0'],[[['withdrawList','',k,'tradeManner']]]]]]]}}" class="pay-li" bindtap="__e"><view class="left"><image class="img" lazy-load="{{true}}" src="{{'../../static/images/'+item.img+'.png'}}" mode="aspectFill"></image><text class="text">{{''+item.title+''}}</text></view><view class="{{['iconfont iconlujing',(tradeManner==item.tradeManner)?'iconxuanzhong':'']}}"></view></view></block></view><form data-event-opts="{{[['submit',[['submit',['$event']]]]]}}" bindsubmit="__e"><view class="mybox"><input class="input" value="" name="realName" type="text" placeholder="请输入姓名"/><block wx:if="{{tradeManner!='2'}}"><input class="input" value="" name="account" type="text" placeholder="请输入账号"/></block><input class="input" value="" name="mobileNo" type="text" placeholder="请输入手机号码" maxlength="11"/></view><view class="mybox"><view class="title-line">提现金额<text class="font-small">{{"（收取"+handlingFee+"%手续费）"}}</text></view><input class="input" name="tradeAmount" type="text" placeholder="{{'输入金额(最少提现'+minWithdraw+'元)'}}" data-event-opts="{{[['input',[['__set_model',['','tradeAmount','$event',[]]]]]]}}" value="{{tradeAmount}}" bindinput="__e"/><view class="tips-box"><view class="tips">{{"当前最多可提取"+$root.f0+"元"}}</view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="allin" bindtap="__e">全部提现</view></view></view><button class="no-btn confirm-btn" formType="submit" loading="{{loading}}">我要提现</button></form></view>