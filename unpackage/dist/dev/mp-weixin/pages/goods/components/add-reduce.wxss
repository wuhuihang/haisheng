@charset "UTF-8";
/* 颜色变量 */
.num-box {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.num-box .num {
  float: left;
  font-size: 26rpx;
  padding: 0 10rpx;
  min-width: 50rpx;
  text-align: center;
  height: 38rpx;
  line-height: 38rpx;
}
.num-box .iconfont {
  width: 38rpx;
  height: 38rpx;
  line-height: 38rpx;
  background: #EEEEEE;
  border-radius: 100%;
  float: left;
  color: #333;
  font-size: 20rpx;
  text-align: center;
  cursor: pointer;
  cursor: pointer;
}

