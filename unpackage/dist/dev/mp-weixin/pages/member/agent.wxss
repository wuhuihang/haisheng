@charset "UTF-8";
/* 颜色变量 */
.info-box {
  width: calc(100% - 68rpx);
  height: 120rpx;
  padding: 34rpx;
  background: white;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.info-box .left {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 34rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.info-box .left .address {
  font-size: 26rpx;
  color: #666666;
}
.info-box .img {
  width: 120rpx;
  height: 120rpx;
  border-radius: 100%;
  margin-right: 24rpx;
  background: #ffa098;
}
.info-box .primary-btn {
  width: 120rpx;
  height: 52rpx;
  border-radius: 26px;
  padding: 0;
  text-align: center;
  line-height: 52rpx;
  background: #ffa098;
  color: white;
  font-size: 26rpx;
}
.order-list {
  width: calc(100% - 108rpx);
  margin: 12rpx auto;
  background: white;
  border-radius: 10rpx;
  padding: 0 20rpx;
  padding-bottom: 24rpx;
  margin-top: 130rpx;
}
.order-list .order-title {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 30rpx;
}
.money-list {
  margin: 30rpx auto !important;
}
.money-list .money-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.money-list .money-box .money-li {
  width: 33.3%;
  text-align: center;
  position: relative;
}
.money-list .money-box .money-li .num {
  width: 100%;
  font-size: 36rpx;
  color: #D14036;
  margin: 30rpx 0;
}
.money-list .money-box .money-li .title {
  font-size: 24rpx;
}
.money-list .money-box .money-li::after {
  position: absolute;
  top: 50%;
  margin-top: -22rpx;
  right: 0;
  z-index: 10;
  content: "";
  width: 1px;
  height: 44rpx;
  background: #C6C6C6;
}
.money-li:last-child::after {
  background: white !important;
}
.open-agent {
  width: 593rpx;
  height: 146rpx;
  margin: 0 auto;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 30rpx;
  padding: 0 50rpx;
  color: #666666;
  font-size: 26rpx;
}
.open-agent .title {
  color: #444444;
  font-size: 30rpx;
  margin-bottom: 10rpx;
}
.income-list .area {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin: 30rpx 0;
}
.income-list .area .border-btn {
  padding: 10rpx 24rpx;
  margin-right: 30rpx;
  border-color: #999999;
  color: #999;
}
.income-list .area .border-btn:last-child {
  margin: 0;
}
.income-list .area .border-btn.active {
  color: #ffa098;
  border-color: #ffa098;
}
.income-list .income-detail {
  margin-top: 30rpx;
}
.income-list .income-detail .agent-income {
  border-top: 1px solid #E6E6E6;
  width: 100%;
  height: 180rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.income-list .income-detail .agent-income .agent-income-li {
  width: 50%;
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-align-content: center;
          align-content: center;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  position: relative;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  font-size: 36rpx;
}
.income-list .income-detail .agent-income .agent-income-li .title {
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #666666;
  margin-bottom: 24rpx;
}
.income-list .income-detail .agent-income .agent-income-li::after {
  position: absolute;
  top: 50%;
  margin-top: -60rpx;
  right: 0;
  z-index: 10;
  content: "";
  width: 1px;
  height: 120rpx;
  background: #E6E6E6;
}
.income-list .income-detail .agent-income .agent-income-li:last-child::after {
  background: white;
}

