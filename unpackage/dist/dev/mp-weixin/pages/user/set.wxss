@charset "UTF-8";
/* 颜色变量 */
.confirm-btn {
  width: calc(100% - 60rpx);
  background: #ffa098;
  margin: 30px auto;
  height: 40px;
  line-height: 40px;
  border-radius: 50px;
  font-size: 30rpx;
  padding: 0;
  font-weight: bold;
  color: white;
  text-align: center;
  cursor: pointer;
}
.box {
  width: 100%;
  min-height: calc(100vh - 0px);
  background: white;
}
.box .set-li {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: calc(100% - 68rpx);
  padding: 0 34rpx;
  height: 88rpx;
  font-size: 30rpx;
}
.logout {
  width: 682rpx;
  margin: 0 auto;
  margin-top: 180rpx;
}

