@charset "UTF-8";
/* 颜色变量 */
.rule {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 20rpx;
}
.rule .iconfont {
  font-size: 30rpx;
  padding: 8rpx;
}
.rule .iconxuanzhong {
  color: #ffa098;
}
.rule .url {
  color: #ffa098;
}
.back-home {
  position: absolute;
  top: 50rpx;
  left: 50rpx;
  color: #000;
  font-size: 30rpx;
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.confirm-btn {
  background: #ffa098;
  margin: 60rpx 0;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 100rpx;
  font-size: 30rpx;
  padding: 0;
  font-weight: bold;
  color: white;
  text-align: center;
  cursor: pointer;
}
.iconshouye-xiaoxi {
  font-size: 32rpx !important;
}
.login-main {
  width: 100%;
}
.bg-primary {
  background: #ffa098;
  margin: 60rpx 0;
}
.box {
  width: 100%;
  height: 100vh;
  background: white;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.box .go-register {
  position: absolute;
  top: 100rpx;
  right: 40rpx;
  font-size: 30rpx;
  color: #ffa098;
}
.login-tips {
  width: calc(100% - 80rpx);
  margin: 0 auto;
  font-size: 60rpx;
  margin-bottom: 70rpx;
}
.login-box {
  width: calc(100% - 80rpx);
  margin: 0 auto;
}
.login-box .login-line {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 26rpx;
}
.login-box .login-line .title {
  width: 130rpx;
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
}
.login-box .login-line .input {
  font-size: 26rpx;
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
          flex-grow: 1;
}
.yzm-btn {
  padding: 12rpx 20rpx;
  margin-left: 10rpx;
  height: 26rpx;
  line-height: 26rpx;
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
}
.other-login {
  width: 100%;
  font-size: 30rpx;
  width: 100%;
  text-align: center;
  color: #a1a1a1;
  margin-top: 60rpx;
}
.other-login .iconfont {
  font-size: 60rpx;
  margin: 0 20rpx;
  margin-top: 40rpx;
}
.other-login .login-type {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}

